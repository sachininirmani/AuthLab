# Copy this file to .env.local and fill values.
# Supabase project URL and anon key
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Optional: For server-side verification, Supabase recommends a service role key.
# ⚠️ Never expose service role key to the browser.
SUPABASE_SERVICE_ROLE_KEY=

# Admin email list (comma separated). These accounts get ADMIN privileges in the lab.
AUTHLAB_ADMIN_EMAILS=

# Secret used only for internal demo cookies (role simulation).
# On Vercel, set as an environment variable.
AUTHLAB_COOKIE_SECRET=
